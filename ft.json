{
    "executionRoleArn": "arn:aws:iam::270354559873:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                  "awslogs-group": "/ecs/FTT",
                  "awslogs-region": "us-west-2",
                  "awslogs-stream-prefix": "ecs"
                }
            },
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ],
            "image": "270354559873.dkr.ecr.us-west-2.amazonaws.com/ecr_registry:294a59ccb2d19e50c30fd98d8c8f14a3c22fd4a5",
            "name": "ft_cont"
        }
    ],
    "memory": "1024",
    "family": "FTT",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256"
   
}